<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
    <!-- Servlet definition --> 
    <servlet>
        <servlet-name>servletType</servlet-name>
        <servlet-class>com.certant.app.controllers.ServletType</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>servletPokemon</servlet-name>
        <servlet-class>com.certant.app.controllers.ServletPokemon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletSearcher</servlet-name>
        <servlet-class>com.certant.app.controllers.ServletSearcher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletList</servlet-name>
        <servlet-class>com.certant.app.controllers.ServletList</servlet-class>
    </servlet>                
    
    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>servletType</servlet-name>
        <url-pattern>/servletType</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>servletPokemon</servlet-name>
        <url-pattern>/servletPokemon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletSearcher</servlet-name>
        <url-pattern>/servletSearcher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletList</servlet-name>
        <url-pattern>/servletList</url-pattern>
    </servlet-mapping>                    
</web-app>
